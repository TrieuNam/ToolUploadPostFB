{
  "manifest_version": 3,
  "name": "TikTok Video Link Extractor",
  "version": "1.0.0",
  "description": "Extract video links from TikTok profiles for automation",
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://www.tiktok.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*"],
      "js": ["tiktok-extractor.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
